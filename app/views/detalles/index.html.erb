<p id="notice"><%= notice %></p>

<h1>Detalles</h1>

<table class="table">
  <thead>
    <tr>
      <th>Cantida</th>
      <th>Prestamos</th>
      <th>Libros</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalles.each do |detalle| %>
      <tr>
        <td><%= detalle.cantida %></td>
        <td><%= detalle.prestamos_id %></td>
        <td><%= detalle.libros_id %></td>
        <td><%= link_to 'Show', detalle %></td>
        <td><%= link_to 'Edit', edit_detalle_path(detalle) %></td>
        <td><%= link_to 'Destroy', detalle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalle', new_detalle_path %>
